{
  "name": "@metafoks/mongodb",
  "version": "2.0.3",
  "description": "Metafoks mongodb extension",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "keywords": [
    "metafoks",
    "extension",
    "mongodb",
    "mongo",
    "db"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --fix --ext .js,.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Meatafoks/MongoDB"
  },
  "author": "Yakov Foks <notafoks@yandex.ru>",
  "license": "MIT",
  "devDependencies": {
    "@metafoks/eslint-config": "1.1.0",
    "@metafoks/prettier": "1.1.0",
    "@types/jest": "29.4.0",
    "@types/node": "^20.11.10",
    "@types/uuid": "^9.0.8",
    "eslint": "8.56.0",
    "eslint-plugin-jest": "^27.4.3",
    "jest": "29.4.3",
    "prettier": "3.2.4",
    "ts-jest": "29.0.5",
    "typescript": "^5.3.3"
  },
  "prettier": "@metafoks/prettier",
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "plugins": [
      "jest"
    ],
    "extends": [
      "@metafoks/eslint-config"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules",
      "<rootDir>/src/index.ts",
      ".mock.ts"
    ],
    "coverageDirectory": "<rootDir>/coverage/",
    "testRegex": "(/test/.*\\.(test|spec))\\.(jsx?|tsx?|ts|js)$",
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node",
      "jsx",
      "tsx"
    ]
  },
  "peerDependencies": {
    "metafoks-application": "^1.0.15"
  },
  "dependencies": {
    "mongodb-memory-server": "9.1.6",
    "mongodb": "6.3.0",
    "uuid": "^9.0.1"
  }
}
